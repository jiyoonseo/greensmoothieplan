<script type="text/ng-template" id="dialogOne.ejs"><md-dialog aria-label="Mango (hello)"  ng-cloak>
<form>
    <md-toolbar>
        <div class="md-toolbar-tools">
            <h2>Date Picker</h2>
            <span flex></span>
            <md-button class="md-icon-button" ng-click="cancel()">
                <ng-md-icon icon="close" aria-label="Close dialog"></ng-md-icon>
            </md-button>
        </div>
    </md-toolbar>

    <md-dialog-content>
        <div class="md-dialog-content">
            <md-datepicker  md-placeholder="Enter date" ng-model="pickDate" ng-change="setNewDate(pickedDate)" class=""></md-datepicker> <!-- ng-model="daily.myDate[i]" ng-change="answer(ingredient.day, i)"  -->
        </div>
    </md-dialog-content>

    <md-dialog-actions layout="row">
        <span flex></span>
        <md-button class="md-raised md-primary md-button md-ink-ripple" ng-click="eachDate(pickDate)">
            Save
        </md-button>
        <!--
        <md-button ng-click="cancel()" style="margin-right:20px;">
            Cancel
        </md-button>
        -->
    </md-dialog-actions>
</form>
</md-dialog>
</script>